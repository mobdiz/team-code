{
  "name": "tests-for-team",
  "version": "1.0.0",
  "description": "Всякие тесты, чтобы поупражняться",
  "main": "index.ts",
  "scripts": {
    "test": "vitest run",
    "test:run": "vitest",
    "test:coverage": "vitest run --coverage.enabled --coverage.exclude=**/**/result/",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "start": "npx vite-node ./index.ts",
    "start:bem": "npx vite-node ./bem/index.ts",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitest/coverage-v8": "^1.6.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "prettier": "3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  }
}
